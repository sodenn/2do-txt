{
  "name": "2do-txt",
  "version": "1.18.0",
  "description": "todo.txt-compatible task manager",
  "license": "GPL-3.0",
  "private": true,
  "author": {
    "name": "sodenn",
    "email": "mail@sodenn.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sodenn/2do-txt.git"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint . --max-warnings=0 --ext .ts,.tsx",
    "fmt:check": "prettier --check ./src",
    "fmt": "prettier --write ./src",
    "hygiene": "run-p typecheck lint fmt:check knip",
    "knip": "knip",
    "typecheck": "tsc --noEmit",
    "cap:sync": "npx cap sync",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:start": "run-s build ios:sync ios:open",
    "ios:res": "cordova-res ios --skip-config --copy",
    "tauri": "tauri",
    "tauri:start": "RUST_BACKTRACE=full npm run tauri dev",
    "semantic-release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.2.2",
    "@capacitor/filesystem": "5.1.1",
    "@capacitor/ios": "5.2.2",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/local-notifications": "5.0.6",
    "@capacitor/network": "5.0.6",
    "@capacitor/preferences": "5.0.6",
    "@capacitor/share": "5.0.6",
    "@capacitor/splash-screen": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@formkit/auto-animate": "0.7.0",
    "@lexical/react": "0.11.3",
    "@lexical/utils": "0.11.3",
    "@mui/icons-material": "5.14.1",
    "@mui/lab": "5.0.0-alpha.137",
    "@mui/material": "5.14.2",
    "@mui/system": "5.14.1",
    "@mui/x-date-pickers": "6.10.1",
    "@tauri-apps/api": "1.4.0",
    "@types/pulltorefreshjs": "0.1.5",
    "capacitor-rate-app": "4.0.3",
    "cordova-plugin-inappbrowser": "5.0.0",
    "cordova-plugin-ios-keychain": "https://github.com/ionic-team/cordova-plugin-ios-keychain.git#adb566bc0acac5c2500496d5507a5eb74689980f",
    "date-fns": "2.30.0",
    "dropbox": "10.34.0",
    "fast-xml-parser": "4.2.6",
    "file-saver": "2.0.5",
    "i18next": "23.2.11",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.1",
    "jszip": "3.10.1",
    "lexical": "0.11.3",
    "lexical-beautiful-mentions": "0.1.9",
    "lodash": "4.17.21",
    "notistack": "3.0.1",
    "pulltorefreshjs": "0.1.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-i18next": "13.0.2",
    "react-movable": "3.0.4",
    "react-router-dom": "6.14.2",
    "typeface-roboto": "1.1.13",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@capacitor/cli": "5.2.2",
    "@playwright/test": "1.36.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tauri-apps/cli": "1.4.0",
    "@testing-library/react": "14.0.0",
    "@types/file-saver": "2.0.5",
    "@types/lodash": "4.14.195",
    "@types/node": "18.17.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cordova-res": "0.15.4",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "10.5.2",
    "knip": "2.16.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "semantic-release": "21.0.7",
    "typescript": "5.1.6",
    "vite": "4.4.6",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.33.0"
  }
}
