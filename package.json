{
  "name": "2do-txt",
  "version": "1.14.0",
  "description": "todo.txt-compatible task manager",
  "license": "GPL-3.0",
  "private": true,
  "packageManager": "yarn@3.2.2",
  "engines": {
    "yarn": ">=3.2.2"
  },
  "author": {
    "name": "sodenn",
    "email": "mail@sodenn.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sodenn/2do-txt.git"
  },
  "dependencies": {
    "@capacitor-community/electron": "4.1.2",
    "@capacitor/clipboard": "4.0.1",
    "@capacitor/core": "4.1.0",
    "@capacitor/filesystem": "4.0.1",
    "@capacitor/ios": "4.1.0",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/local-notifications": "4.0.1",
    "@capacitor/network": "4.0.1",
    "@capacitor/preferences": "4.0.1",
    "@capacitor/share": "4.0.1",
    "@capacitor/splash-screen": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@date-io/date-fns": "2.15.0",
    "@emotion/css": "11.10.0",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.8.4",
    "@mui/lab": "5.0.0-alpha.95",
    "@mui/material": "5.10.1",
    "@mui/x-date-pickers": "5.0.0-beta.5",
    "@react-fluent-edit/core": "1.0.3",
    "@react-fluent-edit/mentions": "1.0.3",
    "@react-fluent-edit/mui": "1.0.3",
    "capacitor-rate-app": "3.0.0",
    "cordova-plugin-inappbrowser": "5.0.0",
    "cordova-plugin-ios-keychain": "https://github.com/ionic-team/cordova-plugin-ios-keychain.git#adb566bc0acac5c2500496d5507a5eb74689980f",
    "crypto-js": "4.1.1",
    "date-fns": "2.29.2",
    "dropbox": "10.32.0",
    "file-saver": "2.0.5",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-http-backend": "1.4.1",
    "jszip": "3.10.1",
    "lodash.throttle": "4.1.1",
    "notistack": "2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.2",
    "react-i18next": "11.18.5",
    "react-movable": "3.0.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "4.0.3",
    "slate": "0.81.3",
    "slate-history": "0.66.0",
    "slate-react": "0.81.0",
    "typeface-roboto": "1.1.13"
  },
  "resolutions": {
    "react-error-overlay": "6.0.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "playwright test",
    "test:record": "playwright codegen localhost:3000",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint ./src --max-warnings=0 --ext .ts,.tsx",
    "format": "prettier --check ./src",
    "resources": "cordova-res ios --skip-config --copy",
    "cap:sync": "npx cap sync && yarn electron:sync",
    "ios:sync": "npx cap sync ios",
    "ios:start": "yarn build && yarn ios:sync && npx cap open ios",
    "ios:res": "cordova-res ios --skip-config --copy",
    "electron:sync": "npx cap sync @capacitor-community/electron",
    "electron:start": "yarn build && yarn electron:sync && npx cap open @capacitor-community/electron",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-movable|remark-parse|mdast-util-from-markdown|mdast-util-to-string|micromark|decode-named-character-reference|character-entities|unist-util-stringify-position|remark-stringify|mdast-util-to-markdown|zwitch|longest-streak|unist-util-visit|unist-util-is|unified|bail|is-plain-obj|trough|vfile)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "4.1.0",
    "@playwright/test": "1.25.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/file-saver": "2.0.5",
    "@types/jest": "28.1.8",
    "@types/node": "16.11.56",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "cordova-res": "0.15.4",
    "import-sort-style-module": "6.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.0",
    "semantic-release": "19.0.5",
    "typescript": "4.8.2"
  }
}
