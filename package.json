{
  "name": "2do-txt",
  "version": "1.18.0",
  "type": "module",
  "description": "todo.txt-compatible task manager",
  "license": "GPL-3.0",
  "private": true,
  "author": {
    "name": "sodenn",
    "email": "mail@sodenn.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sodenn/2do-txt.git"
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build",
    "test": "vitest run",
    "e2e": "playwright test",
    "e2e-update": "playwright test screenshots.spec.ts --update-snapshots",
    "lint": "eslint . --max-warnings=0 --ext .ts,.tsx",
    "fmt:check": "prettier --check ./src",
    "fmt": "prettier --write ./src",
    "hygiene": "run-p typecheck lint fmt:check knip",
    "knip": "knip",
    "typecheck": "tsc --noEmit",
    "cap:sync": "npx cap sync",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:start": "run-s build ios:sync ios:open",
    "ios:res": "cordova-res ios --skip-config --copy",
    "tauri": "tauri",
    "tauri:start": "RUST_BACKTRACE=full npm run tauri dev",
    "semantic-release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/filesystem": "5.1.4",
    "@capacitor/ios": "5.5.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/local-notifications": "5.0.6",
    "@capacitor/network": "5.0.6",
    "@capacitor/preferences": "5.0.6",
    "@capacitor/share": "5.0.6",
    "@capacitor/splash-screen": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/inter": "5.0.15",
    "@formkit/auto-animate": "0.8.1",
    "@lexical/react": "0.12.2",
    "@lexical/utils": "0.12.2",
    "@mui/icons-material": "5.14.16",
    "@mui/joy": "5.0.0-beta.14",
    "@mui/material": "5.14.17",
    "@mui/system": "5.14.17",
    "@mui/x-date-pickers": "6.18.0",
    "@tauri-apps/api": "1.5.1",
    "@types/pulltorefreshjs": "0.1.7",
    "capacitor-rate-app": "4.0.3",
    "cordova-plugin-inappbrowser": "5.0.0",
    "cordova-plugin-ios-keychain": "https://github.com/ionic-team/cordova-plugin-ios-keychain.git#adb566bc0acac5c2500496d5507a5eb74689980f",
    "date-fns": "2.30.0",
    "dropbox": "10.34.0",
    "fast-xml-parser": "4.3.2",
    "file-saver": "2.0.5",
    "i18next": "23.7.7",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.3.1",
    "jszip": "3.10.1",
    "lexical": "0.12.2",
    "lexical-beautiful-mentions": "0.1.19",
    "lodash": "4.17.21",
    "pulltorefreshjs": "0.1.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-i18next": "13.5.0",
    "react-router-dom": "6.18.0",
    "react-transition-group": "4.4.5",
    "sonner": "1.2.0",
    "zustand": "4.4.6"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
    "@playwright/test": "1.40.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tauri-apps/cli": "1.5.6",
    "@testing-library/react": "14.1.2",
    "@types/file-saver": "2.0.7",
    "@types/lodash": "4.14.202",
    "@types/node": "20.9.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-transition-group": "4.4.9",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "cordova-res": "0.15.4",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "12.10.3",
    "knip": "2.42.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "semantic-release": "22.0.8",
    "typescript": "5.3.2",
    "vite": "5.0.2",
    "vitest": "1.0.1"
  }
}
